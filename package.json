  "bugs": {
    "url": "https://github.com/DBCDK/dbc-node-opensearch-client/issues"
  },
  "homepage": "https://github.com/DBCDK/dbc-node-opensearch-client#readme",
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-eslint": "^3.1.9",
    "cache-manager": "^0.18.0",
    "chai": "^2.3.0",
    "eslint": "^0.21.0",
    "mocha": "^2.2.4",
    "mocha-jenkins-reporter": "^0.1.9"
  },
  "dependencies": {
    "dbc-node-basesoap-client": "git+https://github.com/DBCDK/dbc-node-basesoap-client#develop"
